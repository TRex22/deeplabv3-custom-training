{
  "selected_model": "deeplabv3_resnet50", // "deeplabv3_resnet101"
  "dataset": "cityscapes", // "COCO16", "COCO21"
  "opt_function": "ADAM", //"SGD", // "ADAM",
  "batch_size": 8,
  "val_batch_size": 1, // 8 // Val is not cropped // 1 seems to be the fastest
  "outer_batch_size_multiplier": 10, //15, // Used to pre-load data
  "epochs": 50,
  "sample_percentage": 1.0, //0.25 // 0.1 // 1.0
  "lr": 0.03, // Will change over time
  "momentum": 0.8,
  "beta_1": 0.9,
  "beta_2": 0.999,
  "epsilon": 1e-03, // 1e-04, // 1e-08, // Must be larger than 1e-05 for half precision
  "weight_decay": 0,
  "amsgrad": true, // To attempt to fix convergence issues
  "load_model": false,
  "save_path": "/data/trained_models/deeplabv3_resnet50/adam/",
  "dataset_path": "/data/data/cityscapes/data_raw/",
  "cityscapes_mode": "fine", // "coarse"
  "shuffle": true,
  "drop_last": true,
  "pin_memory": true,
  "train_num_workers": 1,
  "val_num_workers": 2, // Validation is smaller images so can fit more in-memory
}
